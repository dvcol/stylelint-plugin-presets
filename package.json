{
  "name": "@dvcol/stylelint-plugin-presets",
  "type": "module",
  "version": "2.1.2",
  "packageManager": "pnpm@10.11.0",
  "description": "Basic presets for stylelint",
  "keywords": [
    "stylelint",
    "stylelintplugin",
    "stylelint-plugin"
  ],
  "license": "MIT",
  "author": "dvcol",
  "homepage": "https://github.com/dvcol/stylelint-plugin-presets",
  "bugs": "https://github.com/dvcol/stylelint-plugin-presets/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvcol/stylelint-plugin-presets.git"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">= 8.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "config"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version",
    "release:changelog": "extract-changelog-release > RELEASE.md"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "postcss": "^8.3.11",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^3.0.0",
    "stylelint": ">=15"
  },
  "peerDependenciesMeta": {
    "postcss": {
      "optional": true
    },
    "postcss-html": {
      "optional": true
    },
    "postcss-less": {
      "optional": true
    },
    "postcss-scss": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "postcss": "^8.4.33",
    "postcss-html": "^1.6.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.2.4",
    "requireindex": "^1.2.0",
    "stylelint": "^16.19.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-group-selectors": "^1.0.10",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3"
  },
  "peerDependenciesConfigs": {
    "scss": {
      "postcss": "^8.3.11",
      "postcss-scss": "^4.0.3",
      "prettier": "^2.6.2",
      "stylelint": ">= 15"
    },
    "vue": {
      "postcss": "^8.3",
      "postcss-html": "^1.4.1",
      "postcss-scss": "^4.0.3",
      "prettier": "^2.6.2",
      "stylelint": ">= 14"
    },
    "less": {
      "postcss": "^8.3.11",
      "postcss-less": "^6.0.0",
      "prettier": "^2.6.2",
      "stylelint": "^14.7.0"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@dvcol/eslint-plugin-presets": "^1.3.3",
    "eslint": "^9.0.0",
    "extract-changelog-release": "^1.0.2",
    "husky": "^9.0.0",
    "standard-version": "^9.5.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "svelte",
        "@babel/plugin-syntax-flow",
        "@babel/plugin-transform-react-jsx"
      ],
      "allowedVersions": {
        "stylelint": ">=15"
      }
    }
  }
}
